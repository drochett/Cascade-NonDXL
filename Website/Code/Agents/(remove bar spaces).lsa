	On Error Goto error_handler
	itemLen = Len(dataStr)
			'Call setImportLogMessage(st + "itemLen = " +CStr(itemLen), "Info")
	If symbol &lt;&gt; "" Then
	pos = Instr(datastr,symbol)'
