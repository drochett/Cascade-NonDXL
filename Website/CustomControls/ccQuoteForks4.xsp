          <xp:image url="/Step4_Bevel-FullTaper-Btm_2.gif" id="image11"></xp:image>
          <xp:br></xp:br>
          <xp:div style="padding-left:2.0em;padding-top:1.0em">
            <xp:label value="${langBean.labelBevel4}" style="font-weight:bold"></xp:label>
          </xp:div>
        </xp:panel>
        <xp:panel styleClass="col-xs-6 col-md-4" id="panel20"
          rendered="#{document1.Taper_Options eq 'Full Top Taper and Polish'}">
          <xp:br></xp:br>
          <xp:image url="/Step4_Bevel-TopChiselTip_5.gif" id="image12"></xp:image>
          <xp:br></xp:br>
          <xp:div style="padding-left:2.0em;padding-top:1.0em">
