        </xp:panel>
        <xp:panel styleClass="col-xs-6 col-md-4" id="panel19"
          rendered="#{document1.Taper_Options eq 'Full Bottom Taper and Polish' || document1.Taper_Options eq 'Two Stage Taper and Polish'}">
