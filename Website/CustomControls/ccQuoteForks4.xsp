          </xp:div>
        </xp:panel>
      </xp:panel>
      <!-- END 1 row 4 column -->
      <xp:comboBox id="comboBox2" value="#{document1.Chisel_Bevel_Options}">
        <xp:selectItems>
          <xp:this.value><![CDATA[#{javascript:
var taperOption = "Choose One";
var taperOption = currentDocument.getItemValueString("Taper_Options") ;

if (taperOption == "Choose One" || taperOption == "" || taperOption == null ) {
		return langBean.getValue("selBevelOptions0") ;
	} else if (taperOption == "Standard Taper") {
		return langBean.getValue("selBevelOptions1") ;
	} else if (taperOption == "Full Top Taper and Polish") {
		return langBean.getValue("selBevelOptions2") ;
	} else if (taperOption == "Full Bottom Taper and Polish") {
		return langBean.getValue("selBevelOptions3and4") ;
	} else if (taperOption == "Two Stage Taper and Polish") {
		return langBean.getValue("selBevelOptions3and4") ;
	} else {
		return langBean.getValue("selBevelOptions0") ;
}
	}]]></xp:this.value>
        </xp:selectItems>
      </xp:comboBox>
    </xp:panel>
