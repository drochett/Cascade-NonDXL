  <!-- START BEVEL OPTIONS -->
  <xp:panel styleClass="row" id="panelBevelOptions1">
    <xp:panel styleClass="col-xs-12 col-md-10" id="panel16">
      <xp:table styleClass="address-table">
        <xp:tr>
          <xp:panel styleClass="tableHeading" tagName="th">
            <xp:this.attrs>
              <xp:attr name="colspan" value="2"></xp:attr>
            </xp:this.attrs>
