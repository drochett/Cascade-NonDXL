            <xp:label value="${langBean.labelBevel1}" style="font-weight:bold"></xp:label>
          </xp:div>
        </xp:panel>
        <xp:panel styleClass="col-xs-6 col-md-4" id="panel5"
          rendered="#{document1.Taper_Options eq 'Standard Taper'}">
          <xp:br></xp:br>
          <xp:image url="/Step4_Bevel-StdTaperwBevel_4.gif" id="image9"></xp:image>
